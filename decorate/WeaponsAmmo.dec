  Flash2:
    PLSG A 3 A_GunFlash("FlashB")
    PLSG B 20 A_ReFire
    Goto Ready
  Flash:
    PLSF A 4 Bright A_Light1
    Goto LightDone
  FlashB:
    PLSF B 4 Bright A_Light1
    Goto LightDone
 }
}
