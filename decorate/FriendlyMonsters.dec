 +FRIENDLY +THRUSPECIES +DONTHARMSPECIES
 Species "FriendlyMonsters"
 dropitem NoItemSpawn 1
 states
 {
  // Will allow this monster to wander around in search of enemies instead of standing like a derp. :P
  Spawn:
    TNT1 A 0 NoDelay ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0)
    DIAB A 0 A_Wander
    DIAB A 0 A_Look
    DIAB A 2 A_Wander
    DIAB A 0 A_Look
    DIAB A 2 A_Wander
    DIAB A 0 A_Look
    DIAB B 2 A_Wander
    DIAB A 0 A_Look
    DIAB B 2 A_Wander
    DIAB A 0 A_Look
    DIAB C 2 A_Wander
    DIAB A 0 A_Look
    DIAB C 2 A_Wander
    DIAB A 0 A_Look
    DIAB D 2 A_Wander
    DIAB A 0 A_Look
    DIAB D 2 A_Wander
    DIAB A 0 A_Look
    DIAB E 2 A_Wander
    DIAB A 0 A_Look
    DIAB E 2 A_Wander
    DIAB A 0 A_Look
    DIAB F 2 A_Wander
    DIAB A 0 A_Look
    DIAB F 2 A_Wander
    goto Spawn+1
  See:
    DIAB A 0 A_JumpIfHealthLower(user_HPBuffRange, "ApplyDesperate")
