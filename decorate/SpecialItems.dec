actor SauronHelmetSpawner : CustomInventory 27327
{
  States
  {
   SpawnHelmet:
    TNT1 A 0 A_SpawnItemEx("SauronHelmet",0,0,0,0,0,0,0,0,0)
    TNT1 A 0 A_FadeOut(1.0)
    Loop
   Spawn:
    TNT1 A 0 
    TNT1 A 0 A_JumpIf( ACS_NamedExecuteWithResult("SauronItemSpawnerCheck") >> 0, "SpawnHelmet" )
    TNT1 A 0 A_FadeOut(1.0)
    Loop
  }
}

actor SauronHelmet : CustomInventory 27333
{
 var int user_theta;
 radius 16
 height 16
 mass 666
 +FLOATBOB
 +INVENTORY.ALWAYSPICKUP
 +INVENTORY.BIGPOWERUP
 scale 0.125
 inventory.icon "SHEMA0"
 Inventory.PickupMessage "\caThis old thing looks like it might have some use!\c-"

 States
 {
  NoMore:
   SHEM A 0 A_FadeOut(1.0)
   Loop

  Spawn:
    TNT1 A 0
    TNT1 A 1 ACS_NamedExecuteAlways("HelmetCopyCheck",0,0,0,0)
    TNT1 A 0 A_JumpIf(args[3] == 1,"NoMore")
    TNT1 A 1 ACS_NamedExecuteAlways("HelmetCopyCheck",0,0,0,0)
    TNT1 A 0 A_JumpIf(args[3] == 1,"NoMore")
    TNT1 A 1 
  SpawnSoundEmit:
    SHEM A 0 A_Stopsound(CHAN_ITEM)
    SHEM A 0 A_Playsound("SauronHelmet/Hum",CHAN_ITEM)
    SHEM A 0 A_SetUserVar("user_theta",0)
  SpawnPhase1:
    SHEM A 1 ACS_NamedExecuteAlways("HelmetCopyCheck",0,0,0,0)
    SHEM A 0 A_JumpIf(args[3] == 1,"NoMore")
    SHEM A 1 
    SHEM A 0 A_SetUserVar("user_theta",user_theta+1)
    SHEM A 0 A_JumpIf(user_theta==70,"SpawnSoundEmit")
    loop

  Use:
   TNT1 A 0
   Stop
  Pickup:
   // Other Things
   TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("CheckPlayerIfDead") == 1,2)
   TNT1 A 0 A_PlaySound("generalequip", 5)
   TNT1 A 0 
   Stop
 }
}

actor SauronGauntletsSpawner : CustomInventory 27329
{
  States
  {
   SpawnDeggaris:
    TNT1 A 0 A_SpawnItemEx("SauronGauntlets",0,0,0,0,0,0,0,0,0)
    TNT1 A 0 A_FadeOut(1.0)
    Loop
   SpawnIllucia:
    TNT1 A 0 A_SpawnItemEx("SauronGauntletsIllucia",0,0,0,0,0,0,0,0,0)
    TNT1 A 0 A_FadeOut(1.0)
    Loop
   SpawnFlora:
    TNT1 A 0 A_SpawnItemEx("SauronGauntletsFlora",0,0,0,0,0,0,0,0,0)
    TNT1 A 0 A_FadeOut(1.0)
    Loop

   Spawn:
    TNT1 A 0 
    TNT1 A 0 A_JumpIf( ACS_NamedExecuteWithResult("SauronItemSpawnerCheck") == 1, "SpawnDeggaris" )
