     A_FireBullets(ACS_NamedExecuteWithResult("GetHitScanXSpread",2), ACS_NamedExecuteWithResult("GetHitScanYSpread",2), 1, ACS_NamedExecuteWithResult("GetPlayerATKDMG",6,1), "BulletPuff");
    }
   }
   MNGG B 0 A_ReFire
   MNGG B 1 // 2
   TNT1 A 0 A_Jump(8,"HyperCoolDown")
   TNT1 A 0 A_Jump(32,"FastCoolDown")
   goto SlowCoolDown
  SlowCoolDown:
   MNGG A 4 // 5
   MNGG B 7 // 8
   MNGG A 10 // 11
   MNGG B 13 // 14
   MNGG A 16 // 17
   goto Ready
  FastCoolDown:
   MNGG A 2
   MNGG B 4
   MNGG A 5
   MNGG B 6
   MNGG A 8
   goto Ready
  HyperCoolDown:
   MNGG A 1
   MNGG B 2
   MNGG A 3
   MNGG B 3
   MNGG A 4
   goto Ready
  Flash:
   MNGF A 3 BRIGHT A_Light1
   MNGF B 3 BRIGHT A_Light0
   Stop
 }
