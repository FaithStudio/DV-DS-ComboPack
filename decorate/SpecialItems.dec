 radius 20
 height 20
 scale 0.44
 mass 666
 +FLOATBOB
 +INVENTORY.ALWAYSPICKUP
 +INVENTORY.BIGPOWERUP
 inventory.icon "FLARA0"
 Inventory.PickupMessage "Picked up \cnFlora's Entire Weapon Set\c-! (you cheater :P)"

