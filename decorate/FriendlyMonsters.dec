actor FArchvile : DSArchvile
{
 -COUNTKILL 
 +FRIENDLY +THRUSPECIES +DONTHARMSPECIES
 Species "FriendlyMonsters"
 dropitem NoItemSpawn 1
 states
 {
  // Will allow this monster to wander around in search of enemies instead of standing like a derp. :P
  Spawn:
    TNT1 A 0 NoDelay ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0)
    VILE A 0 A_Wander
    VILE A 0 A_Look
    VILE A 2 A_Wander
    VILE A 0 A_Look
    VILE A 2 A_Wander
    VILE A 0 A_Look
    VILE B 2 A_Wander
    VILE B 0 A_Look
    VILE B 2 A_Wander
    VILE B 0 A_Look
    VILE C 2 A_Wander
    VILE C 0 A_Look
    VILE C 2 A_Wander
    VILE C 0 A_Look
    VILE D 2 A_Wander
    VILE D 0 A_Look
    VILE D 2 A_Wander
    VILE D 0 A_Look
    VILE E 2 A_Wander
    VILE E 0 A_Look
    VILE E 2 A_Wander
    VILE E 0 A_Look
    VILE F 2 A_Wander
    VILE F 0 A_Look
    VILE F 2 A_Wander
    VILE F 0 A_Look
    goto Spawn+1
  See:
    TNT1 A 0 ACS_NamedExecuteAlways("RefreshStealthInfo",0,0)
    VILE AABBCCDDEEFF 2 A_VileChase
    loop
 }
}

actor FDiabloist : Diabloist
{
