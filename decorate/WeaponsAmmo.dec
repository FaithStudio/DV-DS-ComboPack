	 if (CountInv("SpreaderAura2",AAPTR_PLAYER1) > 0)
	 {
      A_FireProjectile("PlasmaBallPlayer",15,0,0,0,0,0);
      A_FireProjectile("PlasmaBallPlayer",7.5,0,0,0,0,7.5);
      A_FireProjectile("PlasmaBallPlayer",7.5,0,0,0,0,-7.5);
      A_FireProjectile("PlasmaBallPlayer",0,1,0,0,0,0);
      A_FireProjectile("PlasmaBallPlayer",-7.5,0,0,0,0,-7.5);
      A_FireProjectile("PlasmaBallPlayer",-7.5,0,0,0,0,7.5);
      A_FireProjectile("PlasmaBallPlayer",-15,0,0,0,0,0);
	 }
	 else
	 if (CountInv("SpreaderAura1",AAPTR_PLAYER1) > 0)
	 {
      A_FireProjectile("PlasmaBallPlayer",15,0,0,0,0,0);
      A_FireProjectile("PlasmaBallPlayer",0,1,0,0,0,0);
      A_FireProjectile("PlasmaBallPlayer",-15,0,0,0,0,0);
	 }
	 else
	 {
      A_FireProjectile("PlasmaBallPlayer",0,1,0,0,0,0);
	 }
	}
    TNT1 A 0 ACS_NamedExecuteWithResult("CheckWeaponFlash",2,1)
    TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("CheckWeaponFlash",2,0) == 1,"Flash1")
    TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("CheckWeaponFlash",2,0) == 0,"Flash2")
  Flash1:
    PLSG A 3 A_GunFlash("FlashOne")
	TNT1 A 0
 	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 5) { return state("FlashF"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 4) { return state("FlashE"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 3) { return state("FlashD"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 2) { return state("FlashC"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 1) { return state("FlashB"); }
     return state("FlashA");
    }
  Flash2:
    PLSG A 3 A_GunFlash("FlashTwo")
	TNT1 A 0
 	{
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 5) { return state("FlashF"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 4) { return state("FlashE"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 3) { return state("FlashD"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 2) { return state("FlashC"); }
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10023) >= 1) { return state("FlashB"); }
     return state("FlashA");
    }

  FlashA:
    PLSG B 20 A_ReFire
    Goto Ready
  FlashB:
    PLSG B 17 A_ReFire
    Goto Ready
  FlashC:
    PLSG B 14 A_ReFire
    Goto Ready
  FlashD:
    PLSG B 11 A_ReFire
    Goto Ready
  FlashE:
    PLSG B 8 A_ReFire
    Goto Ready
  FlashF:
    PLSG B 5 A_ReFire
    Goto Ready

  FlashOne:
    PLSF A 4 Bright A_Light1
    Goto LightDone
  FlashTwo:
    PLSF B 4 Bright A_Light1
    Goto LightDone
 }
