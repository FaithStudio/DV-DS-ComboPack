// Sceptre Cruft and Stuff ;P
actor ShieldPart // The Main segment of the sceptre-shield which deflects/blocks attacks
{
  Species "Players"
  Health 2147483646
  Painchance 255
  Radius 16 // out of 32
  Height 16 // out of 32
  Scale 0.50
  RenderStyle None // Add
  Alpha 0.0625 // 0.003125
  +BUDDHA
  +DONTBLAST
  +SHOOTABLE
  +NOGRAVITY
  +NOTELEPORT
  +NODAMAGE
  +DONTRIP
  +NOBLOOD
  +NOBLOODDECALS
  +FLOORCLIP
  +REFLECTIVE +AIMREFLECT
  +NOTELESTOMP
  +NOTIMEFREEZE
  States
  {
  Spawn:
    HEXA C 2 bright NoDelay //A_RadiusThrust(256,8,RTF_NOIMPACTDAMAGE,8)
    stop
  Pain.Magic:
    TNT1 A 0 
    stop
  Pain.Sceptre:
    TNT1 A 0 
    stop
  Pain:
    TNT1 A 0 bright A_PlaySound ("SceptreEmpyrea/Reflect") // A_PlaySound ("ForceBarrier/Hit")
    stop
  Death:
    TNT1 A 0
    stop
  }
}

