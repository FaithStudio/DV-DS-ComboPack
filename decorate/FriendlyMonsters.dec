actor FArchvile : DSArchvile
{
 + FRIENDLY
 - COUNTKILL
 dropitem NoItemSpawn 1
 states
 {
  CheckMapSkillSpawn:
    TNT1 A 0 ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0,0,0)
    TNT1 A 1 A_SetUserVar("user_checkmapskill",user_checkmapskill+1)
    Goto Spawn+2
  CheckMapSkillSee:
    TNT1 A 0 ACS_NamedExecuteAlways("MonsterStatsHP_ATK_DEF_SPD",0,0,0,0)
    TNT1 A 1 A_SetUserVar("user_checkmapskill",user_checkmapskill+1)
    Goto See+1
  // Will allow this monster to wander around in search of enemies instead of standing like a derp. :P
  Spawn:
    VILE A 0 A_Wander
    TNT1 A 0 A_JumpIf(user_checkmapskill < 1,"CheckMapSkillSpawn")
    VILE A 0 A_Look
    VILE A 2 A_Wander
    VILE A 0 A_Look
    VILE A 2 A_Wander
    VILE A 0 A_Look
    VILE B 2 A_Wander
    VILE B 0 A_Look
    VILE B 2 A_Wander
    VILE B 0 A_Look
    VILE C 2 A_Wander
    VILE C 0 A_Look
    VILE C 2 A_Wander
    VILE C 0 A_Look
    VILE D 2 A_Wander
    VILE D 0 A_Look
    VILE D 2 A_Wander
    VILE D 0 A_Look
    VILE E 2 A_Wander
    VILE E 0 A_Look
    VILE E 2 A_Wander
    VILE E 0 A_Look
    VILE F 2 A_Wander
    VILE F 0 A_Look
    VILE F 2 A_Wander
    VILE F 0 A_Look
    loop
  See:
    TNT1 A 0 A_JumpIf(user_checkmapskill < 1,"CheckMapSkillSee")
    VILE AABBCCDDEEFF 2 A_VileChase
    loop
 }
}

