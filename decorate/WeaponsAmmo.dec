actor ArchonFist : Weapon
{
   Scale 0.625
   Tag "Archon of Hell's Fist"
   +FLOATBOB
   +NOAUTOAIM
   +INVENTORY.ALWAYSPICKUP
   +WEAPON.NOALERT
   Inventory.PickupMessage "You got the Archon of Hell's Fist!"
   States
   {
   Spawn:
      HLC2 J 0
      HLC2 J 1 bright ACS_ExecuteAlways(873,0,9,0,0)
      Loop
   See:
   Ready: 
      CLAW A 1 A_WeaponReady
      Loop
   Deselect: 
      TNT1 A 0 A_Lower
      CLAW A 1 A_Lower
      Loop
   Select: 
      TNT1 A 0 A_Raise
      CLAW A 1 A_Raise
      Loop
   Fire: 
      CLAW A 0 A_JumpIfNoAmmo(1)
      CLAW B 4 A_GunFlash
      CLAW CDEF 4
      Goto Ready
   AltFire:
      CLAW A 0 
      CLAW A 0 ACS_NamedExecuteAlways("HellcoreFireMode",0,0,1)
      CLAW A 10
      Goto Ready

   StartShockCheck:
      CLAW B 0 A_Jump(192, "FlashChoose")
   StartShock:
      CLAW B 0 A_JumpIf(ACS_NamedExecuteWithResult("HellcoreFireMode",1,1) == 0, "StartShockTriShot")
      CLAW B 0 A_JumpIf(ACS_NamedExecuteWithResult("HellcoreFireMode",1,1) == 1, "StartShockWave")
      CLAW B 0 A_JumpIf(ACS_NamedExecuteWithResult("HellcoreFireMode",1,1) == 2, "StartShockComet")
      Stop

   StartShockTriShot:
      CLAW B 0 A_Light2
      CLAW B 0 A_Playsound ("caco/attack")
      TNT1 A 0 A_PlaySound("crisisattack2",7)
      TNT1 A 0 A_PlaySound("crisisattack",6)
      CLAW B 0 ACS_ExecuteAlways(868,0,400,1,0) // +4% Hellcore Heat, Deal Damage if already above 100% Heat
