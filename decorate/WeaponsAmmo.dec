     A_SetAngle(angle+15);
     A_FireBullets (ACS_NamedExecuteWithResult("GetHitScanXSpread",2), ACS_NamedExecuteWithResult("GetHitScanYSpread",2), 1, ACS_NamedExecuteWithResult("GetPlayerATKDMG",6,1), "BulletPuff",0);
