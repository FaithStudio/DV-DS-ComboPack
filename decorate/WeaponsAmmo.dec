      A_SetAngle(angle+15);
	 }
	 else
	 {
      A_FireBullets(ACS_NamedExecuteWithResult("GetHitScanXSpread",5), ACS_NamedExecuteWithResult("GetHitScanYSpread",5), 2, ACS_NamedExecuteWithResult("GetPlayerATKDMG",7,1), "RepeaterPuffPlayer", 1, 10240);
