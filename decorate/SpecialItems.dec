  Spawn:
   ARMR A 0 bright NoDelay ACS_NamedExecuteAlways("CheckFloraArmorCopies",0,1,0,0)
   "####" # 0 A_SetUserVar("user_looper",0)
  Looper:
   "####" # 0 A_SpawnItemEx("ArmorGlitter", 0, 0, 16, random(-2,2), random(-2,2), random(-2,2) ,64)
   "####" # 0 A_SpawnItemEx("ArmorGlitter", 0, 0, 16, random(-2,2), random(-2,2), random(-2,2) ,128)
   "####" # 0 A_SpawnItemEx("ArmorGlitter", 0, 0, 16, random(-2,2), random(-2,2), random(-2,2) ,192)
   "####" # 1 bright ACS_NamedExecuteAlways("CheckFloraArmorCopies",0,1,0,0)
   "####" # 1 bright ACS_NamedExecuteAlways("CheckFloraArmorCopies",0,1,0,0)
   "####" # 0 A_SetUserVar("user_looper",user_looper+1)
   "####" # 0 A_JumpIf(user_looper==21,"PlaySound")
   Loop
  PlaySound:
   "####" # 0 A_Stopsound(CHAN_ITEM)
   "####" # 0 A_Playsound("FloraArmor/Hum",CHAN_ITEM)
   goto Spawn
  Use:
   TNT1 A 0
   Stop
