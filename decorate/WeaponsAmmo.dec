actor FistDeggaris : Weapon
{
  Game Doom
  Weapon.SelectionOrder 3700
  Weapon.Kickback 100
  +WEAPON.WIMPY_WEAPON
  +WEAPON.MELEEWEAPON
  Tag "Fist"
  Obituary "%o chewed on %k's fist."
  States
  {
  Ready:
    PUNG A 1 A_WeaponReady
    loop
  Deselect:
    PUNG A 1 A_Lower
    loop
  Select: 
    PUNG A 1 A_Raise
    loop
  Fire:
    PUNG A 0 A_JumpIf(ACS_NamedExecuteWithResult("UseStaminaAttack",225,2) == 1, "PunchSuccess")
    goto Ready
  PunchSuccess:
    PUNG B 4
    PUNG C 4 A_CustomPunch(ACS_ExecuteWithResult(899),1,0,"FistPuff",0,0) // A_Punch
    PUNG D 5
    PUNG C 4
    PUNG B 5 A_ReFire
    goto Ready
  }
}

