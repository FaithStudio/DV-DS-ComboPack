class AetheriusBaseTeleGlitter : Actor
{
	Default
	{
		+NOBLOCKMAP +NOGRAVITY +MISSILE +ZDOOMTRANS
		RenderStyle "Add";
		Damage 0;
	}

	States
	{
		Spawn:
			TGLT A 2 BRIGHT;
			TGLT B 2 BRIGHT A_AccTeleGlitter;
			TGLT C 2 BRIGHT;
			TGLT D 2 BRIGHT A_AccTeleGlitter;
			TGLT E 2 BRIGHT;
			Loop;
	}
	
	//----------------------------------------------------------------------------
	//
	// PROC A_AccTeleGlitter
