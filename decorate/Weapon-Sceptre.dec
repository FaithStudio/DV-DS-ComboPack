	{
	 A_WeaponReady;
	}
    Loop

  Deselect:
    SCEM A 1
	{
	 A_SetCrosshair(0);
	 A_Lower; A_Lower;
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 4) { A_Lower; }
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 8) { A_Lower; }
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 12) { A_Lower; }
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 16) { A_Lower; }
     A_TakeInventory("ShieldDefense",1); A_TakeInventory("ShieldDefense2",1); A_TakeInventory("ShieldDefense3",1);
     A_TakeInventory("ShieldSlowdown",1); A_TakeInventory("ShieldSlowdown2",1); A_TakeInventory("ShieldSlowdown3",1);
	}
    Loop

  Select:
    SCEM A 1
	{
	 A_SetCrosshair(8);
	 A_Raise; A_Raise;
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 4) { A_Raise; }
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 8) { A_Raise; }
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 12) { A_Raise; }
	 if (ACS_NamedExecuteWithResult("GetSceptreLevel") >= 16) { A_Raise; }
	}
    Loop

  Spawn:
    SERP C -1
    stop
  }
}
