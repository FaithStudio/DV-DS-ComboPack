actor FistDeggaris : Weapon
{
  Game Doom
  Weapon.SelectionOrder 3700
  Weapon.Kickback 100
  +WEAPON.WIMPY_WEAPON
  +WEAPON.NOALERT
  +WEAPON.MELEEWEAPON
  Tag "Fist"
  Obituary "%o chewed on %k's fist."
  States
  {
  Ready:
    PUNG A 1 A_WeaponReady
    loop
  Deselect:
    TNT1 A 0 A_Lower
    PUNG A 1 A_Lower
    loop
  Select: 
    TNT1 A 0 A_Raise
    PUNG A 1 A_Raise
    loop
  Fire:
    PUNG A 0 A_JumpIf(ACS_NamedExecuteWithResult("UseStaminaAttack",ACS_NamedExecuteWithResult("GetPunchStaminaUse",0),2) == 1, "PunchSuccess")
    goto Ready
  PunchSuccess:
    TNT1 A 0 A_Jump(252,2)
    TNT1 A 0 A_AlertMonsters // Sometimes a swing will be enough to wake up all monsters in the area.
    PUNG B 4 A_PlaySound("fistswingheavy",1)
    TNT1 A 0 
	{
     ACS_NamedExecuteAlways("StartTensionCountDown");
	 if (CountInv("SpreaderAura2",AAPTR_PLAYER1) > 0)
	 {
      // Angle + 15, Pitch + 0
      A_SetAngle(angle+15);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
      // Angle + 7.5, Pitch + 7.5
      A_SetAngle(angle-7.5);
      A_SetPitch(pitch+7.5);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
      // Angle + 7.5, Pitch - 7.5
      A_SetPitch(pitch-15);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
      // Angle + 0, Pitch + 0
      A_SetPitch(pitch+7.5);
      A_SetAngle(angle-7.5);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,0,"FistPuff",0,0);
      // Angle - 7.5, Pitch - 7.5
      A_SetAngle(angle-7.5);
      A_SetPitch(pitch-7.5);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
      // Angle - 7.5, Pitch + 7.5
      A_SetPitch(pitch+15);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
      // Angle - 15, Pitch + 0
      A_SetPitch(pitch-7.5);
      A_SetAngle(angle-7.5);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
      A_SetAngle(angle+15);
	 }
	 else
	 if (CountInv("SpreaderAura1",AAPTR_PLAYER1) > 0)
	 {
      A_SetAngle(angle+15);
      A_CustomPunch(ACS_NamedExecuteWithResult("GetPunchDamage",0),1,CPF_NOTURN,"FistPuff",0,0);
