			if (MapCheck == 1200) Exp2 = (MapCheck - 1200) * 1.04; // +4% Exp Bonus // Prolouge - The Mysterious Cave
			// Earth-type Levels
			if (MapCheck == 1201) Exp2 = (MapCheck - 1200) * 1.00; // Monster Bonus // MAP01 - Entryway Pass
