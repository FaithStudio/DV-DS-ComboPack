      A_FireCustomMissile("BFGBallPlayer", 0, 0, 0, 0, 0, 0);
      A_FireCustomMissile("BFGBallPlayer",-15, 0, 0, 0, 0, 0);
	 }
	 else
	 {
      A_FireCustomMissile("BFGBallPlayer", 0, 0, 0, 0, 0, 0);
	 }
	}
    BFGG B 10
	{
	 if (CountInv("RandomInfiniteAmmo",AAPTR_PLAYER1) > 0) { }
	 else
	 if (ACS_NamedExecuteWithResult("MiscVarCheckDECORATE",10029) >= 2) //(CountInv("BFGAttunement2",AAPTR_PLAYER1) > 0)
	 {
	  A_TakeInventory("Cell",24,TIF_NOTAKEINFINITE,AAPTR_PLAYER1);
	 }
	 else
