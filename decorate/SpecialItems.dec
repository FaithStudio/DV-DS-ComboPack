actor SafeTeaShoes : CustomInventory 27301
{
 var int user_theta;
 var int user_looper;
 radius 20
 height 20
 scale 0.33
 //Alpha 0.1 RenderStyle Add
 mass 666
 +FLOATBOB
 +INVENTORY.ALWAYSPICKUP
 +INVENTORY.BIGPOWERUP
 inventory.icon "ARMRA0"
 Inventory.PickupMessage "Got the \chSafe Tea Shoes\c-!!"

