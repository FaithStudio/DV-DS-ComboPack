   Scale 0.625
   +FLOATBOB
   +INVENTORY.ALWAYSPICKUP
   Inventory.PickupMessage "You got the Diabloist's Hand!"
   States
   {
    Spawn:
      TNT1 A 0 bright
      TNT1 A 1 bright ACS_NamedExecuteAlways("HellcoreWeaponCheckCharacter",0,13,0,0)
      TNT1 A 1 bright ACS_NamedExecuteAlways("HellcoreWeaponCheckCharacter",0,13,0,0)
