actor TheOneRing : CustomInventory 27334
{
 var int user_theta;
 radius 4
 height 4
 mass 666
 +FLOATBOB
 +INVENTORY.ALWAYSPICKUP
 +INVENTORY.BIGPOWERUP
 scale 0.05
 inventory.icon "RINGA0"
 Inventory.PickupMessage "You are now the \cfLord of The Rings\c-!! :D"
 Inventory.PickupSound "getonering"

 States
 {
  NoMore:
   RING A 0 A_FadeOut(1.0)
   Loop

  Spawn:
   TNT1 A 0
   TNT1 A 1 ACS_NamedExecuteAlways("OneRingCopyCheck",0,0,0,0)
   TNT1 A 0 A_JumpIf(args[3] == 1,"NoMore")
   TNT1 A 1 ACS_NamedExecuteAlways("OneRingCopyCheck",0,0,0,0)
   TNT1 A 0 A_JumpIf(args[3] == 1,"NoMore")
  SpawnSoundEmit:
   RING A 0 A_Stopsound(CHAN_ITEM)
   RING A 0 A_Playsound("TheOneRing/Hum",CHAN_ITEM)
   RING A 0 A_SetUserVar("user_theta",0)
  SpawnPhase1:
   RING A 0 ACS_NamedExecuteAlways("OneRingCopyCheck",0,0,0,0)
   RING A 0 A_JumpIf(args[3] == 1,"NoMore")
   RING A 2 bright
   RING A 0 A_SetUserVar("user_theta",user_theta+1)
   RING A 0 A_JumpIf(user_theta==10,"StartSpawnPhase2")
   loop
  StartSpawnPhase2:
   RING A 0 A_SetUserVar("user_theta",0)
  SpawnPhase2:
   RING A 0 ACS_NamedExecuteAlways("OneRingCopyCheck",0,0,0,0)
   RING A 0 A_JumpIf(args[3] == 1,"NoMore")
   RING A 2 bright A_FadeOut(0.05)
   RING A 0 A_SetUserVar("user_theta",user_theta+1)
   RING A 0 A_JumpIf(user_theta==20,"StartSpawnPhase3")
   loop
  StartSpawnPhase3:
   RING A 0 A_SetUserVar("user_theta",0)
  SpawnPhase3:
   RING A 0 ACS_NamedExecuteAlways("OneRingCopyCheck",0,0,0,0)
   RING A 0 A_JumpIf(args[3] == 1,"NoMore")
   RING A 2 bright
   RING A 0 A_SetUserVar("user_theta",user_theta+1)
   RING A 0 A_JumpIf(user_theta==10,"StartSpawnPhase4")
   loop
  StartSpawnPhase4:
   RING A 0 A_SetUserVar("user_theta",0)
  SpawnPhase4:
   RING A 0 ACS_NamedExecuteAlways("OneRingCopyCheck",0,0,0,0)
   RING A 0 A_JumpIf(args[3] == 1,"NoMore")
   RING A 2 bright A_FadeIn(0.05)
   RING A 0 A_SetUserVar("user_theta",user_theta+1)
   RING A 0 A_JumpIf(user_theta==20,"SpawnSoundEmit")
   loop
  Pickup:
   // Other Things
   TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("CheckPlayerIfDead") == 1,2)
   TNT1 A 0 A_PlaySound("generalequip", 5)
   TNT1 A 0 
   Stop
  Use:
   TNT1 A 0
   Stop
 }
}

