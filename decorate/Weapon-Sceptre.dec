{
  Health 2147483646
  Painchance 255
  Radius 30 // 16
  Height 30 // 16
  Scale 0.5
  Alpha 0.75
  RenderStyle Translucent
  Species "Players"
  +NOINTERACTION
  +FLOORCLIP
  +NOTIMEFREEZE
  +REFLECTIVE +AIMREFLECT
  +THRUSPECIES
  +SOLID
  +NOBLOOD
  +NOBLOODDECALS
  +NOTELESTOMP
  +DONTBLAST
  +DONTRIP
  +NOTELEPORT
  +NODAMAGE
  +NOGRAVITY
